<%= form_with model: command, url: todos_url, class: 'todo-Form', data: { controller: 'safe-form' } do |f| %>
  <%= f.text_field :param_todo, placeholder: 'Todoを入力...', autocomplete: :off, class: 'todo-Input', data: { target: 'safe-form.field' } %>
  <%= f.button '追加する', type: :submit, class: 'sw-Button sw-Button-primary' %>
<% end -%>
<%= render partial: 'inline_errors', locals: { dom_id: 'js-new-todo-errors', errors: command.errors } %>
